generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  firstName String
  lastName  String
  left      Int       @default(10)
  createdAt DateTime  @default(now())
  id        Int       @id @default(autoincrement())
  email     String
  Workout   Workout[]
}

model Workout {
  type   WorkoutType
  status WorkoutStatus
  date   String
  time   String
  id     Int           @id @default(autoincrement())
  User   User[]

  @@unique([date, time], name: "Workout.date_time_unique")
}

enum WorkoutStatus {
  Active
  Cancelled
  Over
}

enum WorkoutType {
  PERSONAL
  TEAM
}
