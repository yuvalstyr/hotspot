    "version": "0.2.0",
        "configurations": [
    {
        "type": "pwa-chrome",
        "request": "launch",
        "name": "Debug Chrome on localhost",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/pathto/myapp",
        "sourceMapPathOverrides": {
            "webpack://_N_E/*": "${webRoot}/*"
        }
    },
            {
                "type": "pwa-node",
                "request": "launch",
                "name": "Next: Node",
                "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next"
            }
        ]
}