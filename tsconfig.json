{
  "compilerOptions": {
    "sourceMap": true,
    "module": "ES2020",
    "outDir": "dist",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    // "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "./src"
  ],
  "exclude": [
    "node_modules"
  ],
  
}
