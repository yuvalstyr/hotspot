{
    "compilerOptions": {
      "sourceMap": true,
      "module": "commonjs",
      "target": "ES2018",
      "outDir": "dist",
      "lib": ["esnext", "dom"],
      // "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "strictBindCallApply": true,
      "strictFunctionTypes": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noUnusedParameters": false, 
        
        "plugins": [{ "name": "nexus/typescript-language-service" }]
    },
    "include": ["**/*.ts", "**/*.tsx", ".", "types.d.ts"]
  }
